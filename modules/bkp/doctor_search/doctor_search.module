<?php

use Drupal\Core\Routing\RouteMatchInterface;

/**
 * Implements hook_page_attachments().
 */
function doctor_search_page_attachments(array &$attachments) {
  $route_name = \Drupal::routeMatch()->getRouteName();
  if ($route_name === 'doctor_search.form') {
    $attachments['#attached']['library'][] = 'doctor_search/doctor_search';
  }
}
